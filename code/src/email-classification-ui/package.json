{
  "name": "email-classification-ui",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "ng serve --host 0.0.0.0 --port 4200",
    "start:backend": "cd ../ && python -m uvicorn app:app --reload --host 127.0.0.1 --port 8000",
    "build": "ng build",
    "build:first": "npm install && cd ../ && pip install -r requirements.txt",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/cdk": "^19.2.0",
    "@angular/animations": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "perfect-scrollbar": "1.5.5",    
    "@angular/material": "^19.2.0",
    "bootstrap-material-design": "4.1.3",
    "@angular/router": "^19.2.0",    
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "hammerjs": "2.0.8",
    "jquery": "3.6.0",
    "bootstrap": "4.6.1",
    "bootstrap-notify": "3.1.3",
    "arrive": "2.4.1",
    "moment": "2.29.1",
    "chartist": "0.11.4",
    "classlist.js": "1.1.20150312"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "^19.2.4",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.1.2",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.2",
    "@types/jquery": "3.5.6",
    "@types/chartist": "0.11.1"
  }
}
